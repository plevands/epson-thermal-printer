{
  "name": "@plevands/epson-thermal-printer",
  "version": "0.1.2",
  "description": "Library for Epson thermal printer integration with PDF support and React hooks",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/*.js",
    "dist/*.cjs",
    "dist/*.d.ts",
    "dist/hooks",
    "dist/lib",
    "dist/types",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plevands/epson-thermal-printer.git"
  },
  "bugs": {
    "url": "https://github.com/plevands/epson-thermal-printer/issues"
  },
  "homepage": "https://github.com/plevands/epson-thermal-printer#readme",
  "scripts": {
    "dev": "vite --host --port 5123",
    "build": "tsc -b && vite build",
    "dev:lib": "vite build --watch",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "pdfjs-dist": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "pdfjs-dist": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "keywords": [
    "epson",
    "printer",
    "thermal-printer",
    "epos",
    "pdf",
    "react",
    "typescript"
  ],
  "author": "Colegio Plevand's",
  "license": "MIT"
}
